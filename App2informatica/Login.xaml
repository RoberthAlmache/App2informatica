<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App2informatica.Login">
  
        <StackLayout>
        <Frame BackgroundColor="Aqua" Padding="10" CornerRadius="0">
            <Label Text="Semana 3 Navegacion entre ventanas" HorizontalTextAlignment="Center" TextColor="White" FontSize="Body"></Label>
           </Frame>

        <Entry x:Name="txtUsuario" Placeholder="Ingrese el usuario"/>
        <Entry x:Name="txtContraseña" Placeholder="Ingrese la contraseña" IsPassword="True"/>
            
        <Button x:Name="btnAbrir" Text="Abrir Ventana dos" Clicked="btnAbrir_Clicked"/>
    </StackLayout>
</ContentPage>